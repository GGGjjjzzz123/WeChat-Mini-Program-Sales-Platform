
.search-container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10px;
}

.search-input {
  flex: 1;
  height: 40px;
  padding-left: 10px;
  border: 1px solid #ccc;
  border-radius: 20px;
}

.search-btn {
  margin-left: 10px;
  background-color: #007aff;
  color: white;
  border: none;
  padding: 10px 20px;
  border-radius: 20px;
  font-size: 16px;
}
